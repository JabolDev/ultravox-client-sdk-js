{
  "name": "ultravox-client",
  "version": "0.3.4",
  "private": false,
  "files": [
    "dist",
    "src/noise-suppressor.worklet.js"
  ],
  "tshy": {
    "dialects": [
      "esm"
    ],
    "exports": {
      ".": "./src/index.ts",
      "./package.json": "./package.json",
      "./noise-suppressor.worklet.js": "./src/noise-suppressor.worklet.js",
      "./version": "./src/version.ts"
    }
  },
  "scripts": {
    "prepublishOnly": "node -p \"'export const ULTRAVOX_SDK_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "build": "node -p \"'export const ULTRAVOX_SDK_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts && pnpm run prepare && pnpm run format",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "tshy",
    "serve-example": "http-server"
  },
  "dependencies": {
    "livekit-client": "^2.8.0"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "prettier": "^3.3.3",
    "tshy": "^3.0.2",
    "typescript": "^5.1.3"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  },
  "engines": {
    "pnpm": ">=6.0.0"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json",
    "./noise-suppressor.worklet.js": {
      "import": {
        "types": "./dist/esm/noise-suppressor.worklet.d.ts",
        "default": "./dist/esm/noise-suppressor.worklet.js"
      }
    },
    "./version": {
      "import": {
        "types": "./dist/esm/version.d.ts",
        "default": "./dist/esm/version.js"
      }
    }
  },
  "module": "./dist/esm/index.js"
}
